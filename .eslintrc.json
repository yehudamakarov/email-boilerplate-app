{
    "extends": [
        "airbnb",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            1,
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 80,
                "tabWidth": 4
            }
        ]
    },
    "plugins": [
        "prettier"
    ]
}